<configadmin xmlns="http://jonas.ow2.org/ns/configadmin/1.0">

 <!-- xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jonas.ow2.org/ns/configadmin/1.0
